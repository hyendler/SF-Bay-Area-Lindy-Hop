<h1>Channels</h1>

<div id="navbar">
  <ul>
    <li><a href="/">Home</a></li>
    <% if session[:user_id] %>
    <li><a href="/logout">Logout</a></li>
    <% else %>
    <li><a href="/login">Login</a></li>
    <% end %>
  </ul>
</div>

<ul>
  <% @channels.each do |channel| %>
    <li><a href="/channels/<%=channel.id%>"><%= channel.name%></a></li>
  <% end %>
</ul>